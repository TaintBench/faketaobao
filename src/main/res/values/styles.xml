<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyDialog">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="ListItem.Divider">
        <item name="android:background">@color/K_black_light_6</item>
        <item name="android:focusable">false</item>
        <item name="android:clickable">false</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>
    <style name="TextView.SubTitle">
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">@color/D_black_light_1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="EditText.Normal">
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">@color/D_black_light_1</item>
        <item name="android:textColorHint">@color/G_black_light_5</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">right|center_horizontal|clip_horizontal</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:maxLength">64</item>
    </style>
    <style name="Button.HalfScreenGray">
        <item name="android:background">@drawable/button_bg_normal_gray</item>
    </style>
    <style name="Button.Normal">
        <item name="android:textSize">20dp</item>
        <item name="android:textStyle">@null</item>
        <item name="android:textColor">@drawable/button_text_normal</item>
        <item name="android:ellipsize">end</item>
        <item name="android:background">@drawable/button_bg_normal</item>
        <item name="android:layout_width">116dp</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="Button.HalfScreen">
        <item name="android:layout_width">148dp</item>
    </style>
    <style name="AppBaseTheme">
    </style>
    <style name="AppTheme">
    </style>
</resources>
